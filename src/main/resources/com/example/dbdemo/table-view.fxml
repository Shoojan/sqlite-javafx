<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="com.example.dbdemo.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <HBox spacing="10.0">
      <children>
         <TextField promptText="Name" HBox.hgrow="ALWAYS" />
        <Button onAction="#onHelloButtonClick" text="Add" />
      </children>
   </HBox>
   <TableView>
     <columns>
       <TableColumn prefWidth="75.0" text="Id" />
       <TableColumn prefWidth="150.0" text="Name" />
     </columns>
   </TableView>
</VBox>
